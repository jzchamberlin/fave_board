<div class="page-header">
  <h3>List Following #<%= @list_following.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @list_following.user_id %></dd>

      <dt>Board</dt>
      <dd><%= @list_following.board_id %></dd>

      <dt>List</dt>
      <dd><%= @list_following.list_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/list_followings" class="btn btn-primary">
        Back
      </a>
      <a href="/list_followings/<%= @list_following.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_list_following/<%= @list_following.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
