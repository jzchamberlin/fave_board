<div class="page-header">
  <h3>User Following #<%= @user_following.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Following</dt>
      <dd><%= @user_following.following_id %></dd>

      <dt>User</dt>
      <dd><%= @user_following.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/user_followings" class="btn btn-primary">
        Back
      </a>
      <a href="/user_followings/<%= @user_following.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_user_following/<%= @user_following.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
